<?xml version="1.0"?>
<package>
  <metadata>
    <id>PactNet</id>
    <version>$version$</version>
    <authors>seek.com.au</authors>
    <owners>seek.com.au</owners>
    <licenseUrl>https://github.com/SEEK-Jobs/messagePact-net/blob/master/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/SEEK-Jobs/messagePact-net</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A .NET version of Pact, which enables consumer driven contract testing. NOTE: For version 2 and above, you will need to install one of the following Nuget packages, depending on your target platform and architecture: PactNet.Windows, PactNet.OSX, PactNet.Linux.x86 or PactNet.Linux.x64.</description>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="Newtonsoft.Json" version="10.0.3" />
      </group>
      <group targetFramework="netstandard1.5">
        <dependency id="Newtonsoft.Json" version="10.0.3" />
        <dependency id="System.Diagnostics.Process" version="4.3.0" />
        <dependency id="System.Net.Http" version="4.3.0" />
        <dependency id="System.Runtime.Loader" version="4.3.0" />
        <dependency id="System.Threading.Thread" version="4.3.0" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Net.Http" targetFramework="net45" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="bin\Release\netstandard1.5\PactNet.dll" target="lib\netstandard1.5" />
    <file src="bin\Release\netstandard1.5\PactNet.pdb" target="lib\netstandard1.5" />
    <file src="bin\Release\netstandard1.5\PactNet.xml" target="lib\netstandard1.5" />
    <file src="bin\Release\net45\PactNet.dll" target="lib\net45" />
    <file src="bin\Release\net45\PactNet.pdb" target="lib\net45" />
    <file src="bin\Release\net45\PactNet.xml" target="lib\net45" />
  </files>
</package>
