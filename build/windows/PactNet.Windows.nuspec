  </metadata>
  <files>
    <file src="..\tools\pact-win32\**" target="tools\pact-win32\" exclude="**\*.css" />
    <file src="config.json" target="tools\pact-win32\config.json" />
    <file src="PactNet.Windows.targets" target="build\PactNet.Windows.targets" />
  </files>
</package>
